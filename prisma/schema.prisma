// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Course {
  code        Int    @id
  description String
  year        Int
  type        String
  duration    Int
  url         String

  @@unique([code, year])
}
